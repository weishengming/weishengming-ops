pc
	cd /home/git51/www.99fenqi.com
	svn info，确认自己在哪个分支上
	svn up更新
	打包： cd /home/git51  && zip -r www.99fenqi.com.zip www.99fenqi.com/*			＃每次发布都打新包
	cd /home/git51/fenqifu-ops/fab/www.99fenqi.com
	查看发布 fab -l
	fab deploy_liveHangzhou
	rm -f /home/git51/www.99fenqi.com.zip 											#删除，或者下次打包删除

geinihua
	cd /home/git51/web-geinihua-h5
	git branch 确定自己在哪个分支上
	git pull 更新
	打包 cd /home/git51  && zip -r web-geinihua-h5.zip web-geinihua-h5/* 
	cd /home/git51/fenqifu-ops/fab/web-geinihua-h5
	查看发布 fab -l
	fab deploy_liveHangzhou
	rm -f /home/git51/web-geinihua-h5.zip 											#删除，或者下次打包删除
	
